entry/
    load character data
    timer_aura = 300
    timer_death = 0

TIME_TICK_200MS/
    send packet to wand

TIME_TICK_1S[else]/
    if timer_aura &lt; 300 
        timer_aura++
    if postponed_death 
        timer_death ++

POTION_FINISHED/
    play sound

SPELL_CASTED/
    write to log
    check rule of three
    increase spell number for
    spells withou shield

RULE_OF_THREE/
    play special sound
    
MOVE_CASTED/
    write to log


ATTACK_SUCCEEDED/
    play sound
    increase spell number

ATTACK_FAILED/
    play sound

SPELL_WITHOUT_SHIELD_RECEIVED/
    play sound
    coloured flash, vibro

AURA_CIRCUS/
     timer_aura = 0;

COLLOPORTUS/
    send closing sygnal

ALOHOMORA/
    send opening sygnal

DISSENDIUM/
    send opening radio sygnal

PRIORE_INCANTATEM_RECEIVED/
    send three last appropriate spells

MALFOY_MANOR_DEFENCE/
    if timer_aura &gt; 5 
        play malfoy manor defence sound
